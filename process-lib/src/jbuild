(library
 ((name shexp_process)
  (public_name shexp.process)
  ;; No deps outside of the compiler distribution!
(flags (:standard -safe-string))
  (libraries (base
              unix
              threads
              spawn
              shexp_sexp
              shexp_bigstring
              shexp_bigstring_io
              posixat))
  (preprocess no_preprocessing)))

(jbuild_version 1)
